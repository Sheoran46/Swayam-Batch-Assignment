class Solution {
    static int evenlyDivides(int n) {
        int count=0;
        int temp=n;
        while(temp>0){
            int mod=temp%10;
            boolean result=false;
            if(mod>0)
            result=helper(n, mod);
            if(result) count++;
            temp/=10;
        }
        return count;
    }
    public static boolean helper(int n, int n2){
        return n%n2==0 ? true: false;
    }
}
