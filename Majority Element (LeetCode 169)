class Solution {
    public int majorityElement(int[] nums) {
        int last=nums[0];
        int freq=1;
        for(int i=1;i<nums.length;i++){
            if(nums[i]==last)
            freq++;
            else freq--;
            if(freq==0){
                last=nums[i];
                freq=1;
            }
        }
        return last;
    }
}
